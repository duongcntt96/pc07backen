{
    "builds": [
        {
            "src": "vercel_app/wsgi.py",
            "use": "@vercel/python",
            "config": { "build": "sh build.sh" }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "vercel_app/wsgi.py"
        }
    ]
}
